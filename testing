#test_counter = {1:393, 2:999, 3:23920, 4:999999, 6:11111, 7:10}
test_counter = {1:393, 6:11111, 7:10}

print(test_counter.items())
sorted_counter = sorted(test_counter.items(), key= lambda x: x[1], reverse=True)

for i in range(len(sorted_counter)):
    if i > 4:
        break
    name = sorted_counter[i][0]
    score = sorted_counter[i][1]
    print(f"#{i+1}: {name} - {score} points!")
